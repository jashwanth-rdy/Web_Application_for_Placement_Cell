<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Recruiter</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/rec.css" />
  </head>
  <body>
    <!-- Navbar Start -->

    <%- include('../partials/recnavbar.ejs')%>

    <!-- Navbar End -->

    <!-- <button class="buttonClass">one</button>
    <button class="buttonClass">two</button>
    <button class="buttonClass">three</button>
    <button class="buttonClass">four</button> -->
    <div class="container-fluid main">
      <div
        class="btn-group-vertical groupb"
        role="group"
        aria-label="Vertical button group"
      >
        <button
          onclick="location.href = '/rec/<%- rec.Comp_id %>/newjob'"
          type="button"
          class="gbtn1"
        >
          Post New Job
        </button>
        <button
          onclick="location.href = '/rec/<%- rec.Comp_id %>/postedjobs'"
          type="button"
          class="gbtn"
        >
          View Posted Jobs
        </button>

        <button
          onclick="location.href = '/rec/<%- rec.Comp_id %>/profile'"
          type="button"
          class="gbtn"
        >
          Edit Profile
        </button>
        <button
          type="button"
          onclick="location.href = '/rec/<%- rec.Comp_id %>/changepassword'"
          class="gbtn"
        >
          Change Password
        </button>
        <button
          type="button"
          onclick="location.href = '/rec/login'"
          class="gbtn"
        >
          Logout
        </button>
      </div>
      <img
        src="/images/Screenshot 2023-03-23 at 5.23.41 PM.png"
        class="imag"
        alt=""
      />
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
