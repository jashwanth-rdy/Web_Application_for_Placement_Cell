<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Student</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/japplied.css" />
  </head>
  <body>
    <!-- Navbar Start -->

    <%- include('../partials/stunavbar.ejs')%>

    <!-- Navbar End -->
    
    <div class="container">
      <div><h2 class="heading">Scheduled Rounds</h2></div>
      <% if(eve.length<=0) { %>
         <h4 class="text-center mt-4 mb-4">No Scheduled Rounds</h4>
            <% }else{ %>
      <div class="table-responsive">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Round</th>
              <th scope="col">Date</th>
              <th scope="col">Time</th>
              <th scope="col">Venue</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i=0;i<eve.length;i++){ %>
            <tr>
              <!-- <th scope="row"><%- eve[i].Event_id %></th> -->
              <td><%- eve[i].EveName %></td>
              <td><%- eve[i].EveDate.toLocaleDateString() %></td>
              <td><%- eve[i].EveTime %></td>
              <td><%- eve[i].EveVenue %></td>
            </tr>
            <% } %>
          </tbody>
        </table>
        </div>
        <% } %>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
      crossorigin="anonymous"
    ></script>
    <script>
      (function () {
        "use strict";
        const forms = document.querySelectorAll(".validated-form");
        Array.from(forms).forEach(function (form) {
          form.addEventListener(
            "submit",
            function (event) {
              if (!form.checkValidity()) {
                event.preventDefault();
                event.stopPropagation();
              }
              form.classList.add("was-validated");
            },
            false
          );
        });
      })();
    </script>
    <script>
        function toggleElement(bton,iden) {
      const element = document.getElementById(iden);
      if (element.hasAttribute("style")) {
        element.removeAttribute("style");
      }
    }
    function toggleElements(bton,iden) {
      const element = document.getElementById(iden);
      if (element.hasAttribute("style")) {
        element.removeAttribute("style");
      } else {
        element.setAttribute("style","display:none");
      }
    }
    function toggle(iden) {
      const element = document.getElementById(iden);
      if (element.hasAttribute("style")) {
        element.removeAttribute("style");
      } else {
        element.setAttribute("style","display:none");
      }
    }
    </script>
  </body>
</html>
